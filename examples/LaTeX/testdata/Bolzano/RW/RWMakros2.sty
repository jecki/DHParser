	\par\vspace{1.5\baselineskip}\noindent%
	\vbox{\begin{centering}%
	\textbf{\PdUparnr{#1}\\ #2}\Anmerkungsverweis{#1}\PdUtoc{section}{\protect\numberline{\PdUparnr{#1}.}#2}\par%
	\end{centering}}%
	\markboth{\kopfzeilenfmt{Religionswissenschaft\enspace$\cdot$\enspace Teil\ \dieserteil\enspace$\cdot$\enspace\PdUparnr{#1}}}{\kopfzeilenfmt{Religionswissenschaft\enspace$\cdot$\enspace Teil\ \dieserteil\enspace$\cdot$\enspace\PdUparnr{#1}}}\label{par:\dieserteil:#1}%
	\nobreak\@afterheading\par\nobreak\@afterheading\vspace{0.5\baselineskip}\nobreak\@afterheading}
