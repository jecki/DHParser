# EBNF-Syntax f√ºr Artikel des Mittellateinischen W√∂rterbuchs (MLW)

@ comment         =  /(?:\/\/.*)|(?:\/\*(?:.|\n)*?\*\/)/   # Kommentare im C++ Stil
                                      # ohne das Zeilenende zu beinhalten
@ whitespace      =  /[\t ]*/         # Zeilenspr√ºnge z√§hlen nicht als Leerraum
@ literalws       =  right            # Leerraum nach Literalen wird automatisch entfernt
@ disposable      =  /_\w+$/          # Bezeichner, die mit _ beginnen bleiben im AST anonym

##############################################################################

@ Artikel_error   = ':', 'Hier darf kein (weiterer) Doppelpunkt stehen ¬ª{1}¬´, ggf. sollte er in einen (verschachtelten) Kommentar verschoben werden.'
@ Artikel_error   = /\s*ETYMOLOGIE/, 'Die Etymologie-Position muss unmittelbar auf die Grammatik-Position folgen.'
@ Artikel_error   = /\s*[A-Z][A-Z][A-Z]+/, 'An dieser Stelle sollte {0} folgen, nicht ¬ª{1}¬´'
# @ Artikel_skip    = /(?:\n\s*[A-Z][A-Z][A-Z]+)(?:.|\n)*?(?=\n\s*BEDEUTUNG|VERWEISE|AUTOR|STELLENVERZEICHNIS)/, /$/
@ Artikel_skip    = /(?=\n\s*[A-Z][A-Z][A-Z]+)/, /$/

Artikel           = VerweisArtikel |
                    ( [LZ] [NachtragsPosition]
                      ¬ß{ LemmaPosition }+
                      [EtymologiePosition]
                      [VERBOTENE_GRAMMATIKPOSITION]
                      ArtikelKopf
                      [BedeutungsPosition]
                      [VerweisPosition]
                      { UnterArtikel }
                      ArtikelVerfasser
                      [Stellenverzeichnis]
                      [Direktiven]
                      [LZ] DATEI_ENDE )

VERBOTENE_GRAMMATIKPOSITION = GrammatikPosition

#### VERWEIS-ARTIKEL #########################################################

@VerweisArtikel_skip    = /(?=$)/
VerweisArtikel     = &ist_Verweisartikel _VerweisBl√∂cke ¬ß ArtikelVerfasser [Direktiven] [LZ] ¬ß DATEI_ENDE
ist_Verweisartikel = [LZ] "LEMMA" (VerweisBlock | unberechtigt)

@ _VerweisBl√∂cke_resume = /(?=\nAUTOR|$)/
@ _VerweisBl√∂cke_skip   = /(?=LEMMA)/

_VerweisBl√∂cke    = { [LZ] ("LEMMA" ¬ß (VerweisBlock | unberechtigt) { PraeterBlock } | FehlerhafterVerweis) [VERBINDE|TRENNE] }+
  VERBINDE        = &/[^\n]*LEMMA/
  TRENNE          = &L√úCKE

FehlerhafterVerweis = [LZ] "[" "LEMMA" /(?:.|\n)*?(?:\n(?=LEMMA)|(?=\n\s*AUTOR))/

PraeterBlock      = [LL] PRAETER ¬ß ( [VERBINDE|TRENNE] VerweisBlock | unberechtigt [VERBINDE|TRENNE] )
                                   { [LZ] !"LEMMA" ( VerweisBlock | unberechtigt) [VERBINDE|TRENNE] }
@ unberechtigt_skip = /\]/
unberechtigt      = "[" VerweisBlock ¬ß "]"

@ VerweisBlock_error = /\[/, 'Die ¬ªunberechtigt¬´-Markierung [] muss immer am Anfang, ggf. noch vor der Lemma-Nr. stehen!'
@ VerweisBlock_skip = /(?=\w|\n|$)/
@ VerweisBlock_resume = /(?=LEMMA|AUTOR|$)/

VerweisBlock      = AusgangsLemmata Vide
AusgangsLemmata   = _AusgangsLemma { ET _AusgangsLemma }
_AusgangsLemma    = _LemmaSt√ºck {KOMMA _LemmaSt√ºck }
_LemmaSt√ºck       = [LemmaNr ¬ß !unberechtigt] [LZ] {Zusatz [LL]} Lemma [KLAMMER_AUF LemmaWort KLAMMER_ZU] [Zusatz]

@ Vide_resume     = /(?=\nAUTOR|[A-Z][A-Z][A-Z]|\]|$)/
@ Vide_skip       = /(?=ET|\nAUTOR|[A-Z][A-Z][A-Z]|\]|$)/
@ Vide_error      = /\s*{/, "Ein Zusatz ist an dieser Stelle nicht erlaubt: ¬ª{1}¬´"

Vide              = [LL] VIDE [LL]
                    ¬ß ( EinzelVerweis | Zusatz &Vide_Anschluss | VerweisLemmazeile { ET VerweisLemmazeile } )
                    &Vide_Anschluss
Vide_Anschluss    = ZEILENSPRUNG | /\s*\]|\s*$/ | "PRAETER" | "LEMMA"
VerweisLemmazeile = [LemmaNr] ZielLemma [dot] [EinzelVerweis] [!VLZ_Fehler Zusatz] [":" [LZ] Belege]
  dot             =  /\./~
ZielLemma         = ([nicht_klassisch_verweis] ¬∞ [nicht_gesichert]) { Zusatz } ZielLemmaWort
ZielLemmaWort     = _lemma_text

VIDE              = "VIDE" | VIDE_Fehler
ET                = [LZ] ("ET" | ET_Fehler) [LZ]
PRAETER           = "PRAETER" | PRAETER_Fehler

VLZ_Fehler        = ET_Fehler | PRAETER_Fehler | VIDE_Fehler
ET_Fehler         = [`{`] ["/"] ("Et" | "et") [`}`] | `{` ["/"] "ET" `}`
PRAETER_Fehler    = [`{`] ["/"] ("Praeter" | "praeter") [`}`] | `{` ["/"] "PRAETER" `}`
VIDE_Fehler       = [`{`] ["/"] ("Vide" | "vide") [`}`] | `{` ["/"] "VIDE" `}`


#### UNTER-ARTIKEL ###########################################################

UnterArtikel      = ZWW SubLemmaPosition
                    ¬ß[EtymologiePosition]
                    ArtikelKopf
                    BedeutungsPosition
                    [VerweisPosition]

@ SubLemmaPosition_resume = /(?=\nETYMOLOGIE|\nSCHREIBWEISE|\nSTRUKTUR|\nGEBRAUCH|\nMETRIK|\nVERWECHSELBAR|\nBEDEUTUNG)/

SubLemmaPosition  = "SUB_LEMMA" LemmaBlock { [LZ] "VEL" LemmaBlock } ¬ß&({/\s+/~} /[A-Z][A-Z][A-Z]/)

#### NACHTRAG ################################################################

@ NachtragsPosition_resume = /(?=\nLEMMA)/
@ NachtragsPosition_error = '', 'ad oder post -Klausel fehlerhaft oder fehlend: ¬ª{1}¬´'
@ Nachtrag_error   = '', 'Doppelpunkt, ggf. gefolgt von einer Ziffer erwartet, aber nicht ¬ª{1}¬´'
@ ad_error         = '', 'Nachtragslemma oder Nachtragsstellenangabe fehlt oder ist nicht richtig formatiert: ¬ª{1}¬´'
@ post_error       = '', 'Nachtragslemma oder Nachtragsstellenangabe fehlt oder ist nicht richtig formatiert: ¬ª{1}¬´'

NachtragsPosition  = "NACHTRAG" ¬ßNachtrag &({/\s+/~} "LEMMA")

Nachtrag           = [ad ";"] post ¬ß":" [LZ] [Ziffer]
ad                 = "ad" ¬ß_nachtrags_bezug
post               = "post" ¬ß_nachtrags_bezug
_nachtrags_bezug   = (NachtragsLemma [NachtragsStelle] | NachtragsStelle)

NachtragsLemma     = !"p." [LemmaNr <-&/\s*\./] _Lemma
Ziffer             = /\d+\./~
NachtragsStelle    = STELLENTEXT


#### LEMMA-POSITION ##########################################################

@ LemmaPosition_error = '', 'Artikelkopf, Etymologie- oder Bedeutungsposition erwartet, aber nicht ¬ª{1}¬´!'
@ LemmaPosition_resume = /(?=\n\s*ETYMOLOGIE|\n\s*SCHREIBWEISE|\n\s*STRUKTUR|\n\s*GEBRAUCH|\n\s*METRIK|\n\s*VERWECHSELBAR|\n\s*BEDEUTUNG)/

LemmaPosition     = [ABS] "LEMMA" [unecht] LemmaBlock { [LZ] "VEL" LemmaBlock } [GrammatikPosition]
                    ¬ß&(({/\s+/~} /[A-Z][A-Z][A-Z]/) | DATEI_ENDE )
unecht            = "UNECHT"
LemmaNr           = /\d+\.?/~

@ LemmaBlock_error = '', '¬ª{1}¬´ ist kein g√ºltiges Lemma!'
LemmaBlock        = [LZ] {Zusatz [LL]} [LemmaNr]
                    ¬ßLemma ([[LL] Zusatz] ¬∞ [[LL] LemmaVarianten])
                    [verbotener_Punkt]
                    [GrammatikPosition|EtymologiePosition]
  verbotener_Punkt = "."

Lemma             = _Lemma
_Lemma            = ([nicht_klassisch] ¬∞ [nicht_gesichert]) { Zusatz [LL] } LemmaWort

  nicht_klassisch = "*"    # Sechszackig fett ‚ú∂  # Stern 1. Ordnung
  nicht_gesichert = "?"

LemmaVarianten    = "(" LemmaVariante { "," [ZW] LemmaVariante } ")"
LemmaVariante     = /\-?/ _lemma_text [[LL] Zusatz]
LemmaWort         = _lemma_text
_lemma_text       = [/-/] [/\(/ _LATEIN_KLEIN_FOLGE /\)/ | /\(-\)/]
                    LAT_GWORT [/\(-\)/]
                    { [AUSGEWICHEN] !`p.` LAT_WORT [/\(-\)/] }   # Alternativ: ZEILENSPRUNG|AUSGEWICHEN


## GRAMMATIK-POSITION ##

GrammatikPosition = ZWW "GRAMMATIK" ¬ß [LZ] Grammatik [GrammatikVarianten] {[LL] Zusatz}

@ Grammatik_skip  = /[^\w.-] */
@ Grammatik_error = "nomen", '¬ªnomen¬´ ist als wortangabe nicht erlaubt, verwende bitte ¬ªsubstantiv¬´!'

Grammatik         = ¬ßGrammatikAngabe { [LL] vel [LL] [Zus√§tze [LL]] GrammatikAngabe }
@ GrammatikAngabe_error  = ",", 'Nach der Wortartangabe sollte ein Semikolon oder ein Zeilenwechsel folgen, kein Komma!'
@ GrammatikAngabe_resume = /(?=\n)/
GrammatikAngabe   = (wortarten ¬ß&ABS | WA_UNBEKANNT)
                    [ABS [_casus_nummerus] stammformen]
                    [[ABS|VERBOTENES_KOMMA] genera ]
                    [[ABS] AlternativeGrammatik]
AlternativeGrammatik = "(" [Zusatz] [wortarten [Zusatz] [ABS]]
                             [[_casus_nummerus [Zusatz]] stammformen [Zusatz]]
                             [[ABS|VERBOTENES_KOMMA] genera [Zusatz]] ")"

@wortarten_error  = 'Auf ¬ªvel¬´ darf an dieser Stelle nur ¬ªraro¬´, ¬ªrarius¬´, ¬ªsemel¬´, ein Zusatz '
                    'oder gleich eine weitere Wortart folgen, aber nicht ¬ª{1}¬´!'

wortarten         = wortart { [","] [LL] vel [LL] [ Zus√§tze ] [LL] ¬ß wortart }
@ wortart_resume  = /(?=(?:[-:;\n{]|vel|raro|rarius|semel))/
wortart           = "adverbium" | "adv."
                  | "adiectivum" ¬ßnom_klasse | "adi." ¬ßnom_klasse
                  | "comparativus" ¬ßnom_klasse | "compar." ¬ßnom_klasse
                  | "interiectio" [wa_erg√§nzung] | "interi." [wa_erg√§nzung]
                  | "littera" [wa_erg√§nzung]
                  | "numerale" ¬ßnom_klasse | "numer." ¬ßnom_klasse
                  | "particula"
                  | "praepositio" | "praep."
                  | "pronomen" ¬ßnom_klasse | "pron." ¬ßnom_klasse
                  | "substantivum" ¬ßnom_klasse | "subst." ¬ßnom_klasse
                  | "superlativus" ¬ßnom_klasse | "superl." ¬ßnom_klasse
                  | "verbum" ¬ßverb_klasse

WA_UNBEKANNT      = /\w+\.?/ { ~ /\w+\.?/ }

wa_erg√§nzung      = { [ZW] (EINZEILER | Zusatz | Verweise) }+

nom_klasse        = ("IV" | "V" | "III" | "II" | "I-II" | "I"
                    | "anormal" | "unbestimmt")
                    [VERBOTENER_PUNKT]
verb_klasse       = ("IV" | "III" | "II" | "I" | "anormal" | "unbestimmt") [VERBOTENER_PUNKT]

VERBOTENER_PUNKT  = "."
VERBOTENES_KOMMA  = ","

stammformen      = flexion { vel flexion }
flexion          = indeclinabile | FLEX {"," FLEX}
indeclinabile    = "indecl." | "indeclinabile"
FLEX             = { !(genus|wortart|casus|numerus|vel) /-?[a-z]+(?:\([a-z]+\)[a-z]+)*/~ }+

@genera_error    = 'Auf ¬ªvel¬´ darf an dieser Stelle nur ¬ªraro¬´, , ¬ªrarius¬´, ¬ªsemel¬´, ¬ªbis¬´, '
                   'ein Zusatz oder gleich ein weiterer genus folgen, aber nicht ¬ª{1}¬´!'
@genera_resume   = /\.|um\n/

genera           = [numerus] genus { [","] [LL] vel [LL] [Zus√§tze] [LL] ¬ß genus }
genus            = "maskulinum" | "m." | "masc."
                 | "femininum" | "f." | "fem."
                 | "neutrum" | "n." | "neutr."

_casus_nummerus  = casus [numerus] | "{" casus [numerus] ¬ß"}"

casus            = "nominativus" | "nom."
                 | "genitivus" | "gen."
                 | "dativus" | "dat."
                 | "accusativus" | "acc."
                 | "ablativus" | "abl."
                 | "vocativus" | "voc."

numerus          = "singular" | "sg." | "sing."
                 | "plural" | "pl." | "plur."

vel              = "{" _vel ¬ß"}" | _vel
_vel             = (("vel"|ET_VERBOTEN) [LL] [_vel_qualifikator] | VEL_FEHLER)
                   [`(`~ Verweise ¬ß`)`~ | Zusatz]
_vel_qualifikator = "raro" | "rarius" | "semel" | "bis"
ET_VERBOTEN       = "et"
VEL_FEHLER        = "VEL" [LL] _vel_qualifikator | <-!/ *\n/ "VEL"

@ GrammatikVariante_error = "", "Fehlerhafte Angabe bei Grammatik-Variante oder fehlende Belege!"
@ GrammatikVariante_resume = /(?=\n(?=(?:[^\n]*:)|(?:\s*\n)))/

GrammatikVarianten = { LABS GrammatikVariante }+
GrammatikVariante  = GVariante ¬ßDPP Belege
  GVariante = &(wortarten|casus|flexion|genus|numerus)
              ([wortarten [ABS]] ¬∞ [_casus_nummerus] ¬∞ [stammformen [ABS]] ¬∞ [genera [ABS]])


## ETYMOLOGIE-POSITION ##

@ EtymologiePosition_resume = /(?=\nSCHREIBWEISE|\nSTRUKTUR|\nGEBRAUCH|\nMETRIK|\nVERWECHSELBAR|\nBEDEUTUNG)/
@ EtymologiePosition_error = /\./, 'Hier scheint ein Punk zu viel zu sein: {1}'

EtymologiePosition = ZWW "ETYMOLOGIE" (&(ZWW GROSSBUCHSTABEN) | [LZ]
                     ¬ß EtymologieAngabe { _√úberleitung EtymologieAngabe } &(~/\n/))
_√úberleitung        = ZLL | (KOMMA|SEMIKOLON) [ZWW] | &`{`

@ EtymologieAngabe_resume = /(?=\n(?!\s*\*))/
@ EtymologieAngabe_skip = /(?=\n(?!\s*\*))|(?=\n\s*\*)/
@ EtymologieAngabe_error = '', 'Sekund√§rliteratur erwartet, aber ¬ª{1}¬´ nicht als solche erkannt. M√∂glicherweise ist der Titel falsch oder wurde dem Programm noch nicht bekannt gemacht.'
EtymologieAngabe   = ([unsicher] [Zusatz] [ZWW] (EtymologieSprache [Etymon] | Etymon) | Zusatz )
                     [Zusatz &(":" | _√úberleitung | /\s*$/)]
                     [":" ¬ß[ZWW] ( Sekund√§rliteraturBlock | ECKIGE_AUF_KURSIV Sekund√§rliteraturBlock ECKIGE_ZU_KURSIV )]
Sekund√§rliteraturBlock = { Zusatz } [ `*` / */ ] {Zusatz} [ZW] (Sekund√§rliteratur | Verweise)
                          { [ZWW] "*" {Zusatz} [ZW] (Sekund√§rliteratur | Verweise) }
Etymon             = [LemmaNr] ([nicht_klassisch_verweis] ¬∞ [erschlossen] ¬∞ [nicht_gesichert]) { ETYMOLOGIE_TEXT }+ [Anker]
# { (ETYMOLOGIE_TEXT | Kursiv) ([Kursiv] ¬∞ [Zusatz]) }+

EtymologieSprache  = ("anglosax." ["vet."] | "arab." | "bohem." ["vet."]
                     | "byz." | "catal." | "finnice"
                     | "franc." ["vet." | "inf." "vet."] | "francog." ["vet."]
                     | "francoprov."
                     | "frisic." | "gall." | "germ." | "got." | "gr." ["byz."]
                     | "hebr." | "hibern." | "hisp." ["vet."] | "hung." | "ital."
                     # | "lat."     # | "lat-klass." | "lat-mlw." | "lat-erschlossen"
                     | "occ." ["vet."] | "pers." | "polon." ["vet."]
                     | "port." | "prov." | "raetoroman."
                     | "saxon" ["vet."]
                     | "sard." | "sicil." | "slav." | "theod." ["inf."] ["vet."]
                     | "turc."
                     | "val." | fehlerhafte_Sprachangabe ) [fehlerhaftes_vet]
fehlerhafte_Sprachangabe = "graec." | "griech."  # unbedingt nur bestimmte auflisten!!!
fehlerhaftes_vet = { "inf." | "vet." }+

erschlossen = "+"  # f√ºnfzackig d√ºnn Stern 3. Ordnung etymologischer Erschlie√üungsstern: keine Belege

# nat√ºrlicher Stern: Chart. Tirol. 713*!!!

# (ital. ficcare ex *figicare; cf. Battaglia, Dizionario V. p. 934 et Wartburg, Frz. etym. Wb. III. p. 511)

ECKIGE_AUF_KURSIV = "["
ECKIGE_ZU_KURSIV  = "]"


#### ARTIKEL-KOPF ############################################################

ArtikelKopf     = [SchreibweisenPosition]
                ¬∞ [StrukturPosition]
                ¬∞ [FormPosition]
                ¬∞ [GebrauchsPosition]
                ¬∞ [MetrikPosition]
                ¬∞ [VerwechselungsPosition]

SchreibweisenPosition = ZWW "SCHREIBWEISE" Position
StrukturPosition      = ZWW "STRUKTUR"     Position
FormPosition          = ZWW "FORM"         Position
GebrauchsPosition     = ZWW "GEBRAUCH"     Position
MetrikPosition        = ZWW "METRIK"       Position
VerwechselungsPosition = ZWW ("VERWECHSELBARKEIT" | "VERWECHSELBAR") Position

## ARTIKELKOPF POSITIONEN ##

@ Position_error  = /(?!\s*\n)/, "Zeilenwechsel nach Schl√ºsselwort vor ¬ª{1}¬´ erforderlich!"
@ Position_resume = /(?=\n *[A-Z][A-Z][A-Z])/     # Fortf√ºhrung nach Fehler beim n√§chsten Schl√ºsselwort

Position       = &(ZWW GROSSBUCHSTABEN)     # Position darf leer bleiben (Warnmeldung?)
               | ¬ß &ZWW Besonderheiten
                 &(ZWW GROSSBUCHSTABEN)     # Auf eine Position muss immer eine andere Position
                                            # oder ein Hauptstrukturblock folgen.

@ Besonderheit_error = /\n* +\w/, "Einr√ºckung scheint nicht zu stimmen. Zu viele oder zu wenig Leerzeichen!"
@ Besonderheit_error = "Hier sollten Verweise ¬ª{{=> ...}}¬´, Belege oder Unterkategorien folgen, nicht ¬ª{1}¬´. M√∂glicherweise wurde die maximale Gliederungstiefe von vier Ebenen √ºberschritten."

Besonderheiten  = Besonderheit { Besonderheit }  [&:?ER1]    # [&:?ER1] = delete captured indentation sizes
Besonderheit    = TAB_1 Beschreibung ":" ¬ß(Kategorien | [LZ] Belege | Zusatz | fehlende_Belege)
Kategorien      = { TAB_2 Variante | Kategorie }+  [&:?ER2]
Kategorie       = TAB_2 Beschreibung ":" (Unterkategorien | KVarianten | fehlende_Belege)
KVarianten      = { TAB_3 Variante }+
Unterkategorien = { TAB_3 Variante | Unterkategorie }+  [&:?ER3]
Unterkategorie  = TAB_3 Beschreibung ":" (Varianten | fehlende_Belege)
Varianten       = { TAB_4 Variante }+ [&:?ER4]
Variante        = !KATEGORIENZEILE Beschreibung ":" ¬ß( [LZ] Belege | Zusatz | fehlende_Belege)

fehlende_Belege = /(?=\n)/

# Definition von Einr√ºckungstiefen
TAB_1           = ZWS :ER1
TAB_2           = ZWS :ER1 :ER2
TAB_3           = ZWS :ER1 :ER2 :ER3
TAB_4           = ZWS :ER1 :ER2 :ER3 :ER4

ZWS             = { ~/\n/ }+   # Leerzeilen, bis zum Anfang der n√§chsten Zeile

ER1             = / */
ER2             = / +/
ER3             = / +/
ER4             = / +/

@ Beschreibung_resume = /(?=:)/    # Fortf√ºhrung ab Doppelpunkt

Beschreibung   = !/[A-Z][A-Z_][A-Z_]+/ { (EINZEILER | Lemmawort) ¬∞ {Zusatz} }+ {";" Erg√§nzung}
                 { Einschub } &":"   # [Kursiv]  sollte hier nicht n√∂tig sein!
Erg√§nzung      = [Zusatz] { EINZEILER ¬∞ [Zusatz] }+


#### BEDEUTUNGS-POSITION #####################################################

BedeutungsPosition   = { [ABSATZ] Bedeutung }+

@ Bedeutung_resume   = /(?=\n *BEDEUTUNG|\n *ABSATZ|\n *SUB_LEMMA|\n *VERWEISE|\n *AUTOR|$)/  # \n *ANH√ÑNGER|
@ U1Bedeutung_resume = /(?=\n *BEDEUTUNG|\n *ABSATZ|\n *UNTER_B|\n *U_B|\n *U1_B|\n *SUB_LEMMA|\n *VERWEISE|\n *AUTOR|$)/
@ U2Bedeutung_resume = /(?=\n *BEDEUTUNG|\n *ABSATZ|\n *UNTER_B|\n *U_B|\n *U1_B|\n *UNTER_UNTER_B|\n *UU_B|\n *U2_B|\n *SUB_LEMMA|\n *VERWEISE|\n *AUTOR|$)/
@ U3Bedeutung_resume = /(?=\n *BEDEUTUNG|\n *ABSATZ|\n *UNTER_B|\n *U_B|\n *U1_B|\n *UNTER_UNTER_B|\n *UU_B|\n *U2_B|\n *UNTER_UNTER_UNTER_B|\n *UUU_B|\n *U3_B|\n *SUB_LEMMA|\n *VERWEISE|\n *AUTOR|$)/
@ U4Bedeutung_resume = /(?=\n *BEDEUTUNG|\n *ABSATZ|\n *UNTER_B|\n *U_B|\n *U1_B|\n *UNTER_UNTER_B|\n *UU_B|\n *U2_B|\n *UNTER_UNTER_UNTER_B|\n *UUU_B|\n *U3_B|\n *UUUU_B|\n *U4_B|\n *SUB_LEMMA|\n *VERWEISE|\n *AUTOR|$)/
@ U5Bedeutung_resume = /(?=\n *BEDEUTUNG|\n *ABSATZ|\n *UNTER_B|\n *U_B|\n *U1_B|\n *UNTER_UNTER_B|\n *UU_B|\n *U2_B|\n *UNTER_UNTER_UNTER_B|\n *UUU_B|\n *U3_B|\n *UUUU_B|\n *U4_B|\n *UUUUU_B|\n *U5_B|\n *SUB_LEMMA|\n *VERWEISE|\n *AUTOR|$)/

Bedeutung            = ZWW "BEDEUTUNG" [LZ]
                       ¬ßBedeutungsangabe (BelegeOderAufz√§hlung|{[ABSATZ] U1Bedeutung}+|FehlendeBelege) &Anschluss
U1Bedeutung          = ZWW ("U_BEDEUTUNG" | "UNTER_BEDEUTUNG" | "U1_BEDEUTUNG") [LZ]
                       ¬ßBedeutungsangabe (BelegeOderAufz√§hlung|{[ABSATZ] U2Bedeutung}+|FehlendeBelege) &Anschluss
U2Bedeutung          = ZWW ("UU_BEDEUTUNG" | "UNTER_UNTER_BEDEUTUNG" | "U2_BEDEUTUNG") [LZ]
                       ¬ßBedeutungsangabe (BelegeOderAufz√§hlung|{[VERBOTENER_ABSATZ] U3Bedeutung}+|FehlendeBelege) &Anschluss
U3Bedeutung          = ZWW ("UUU_BEDEUTUNG" | "UNTER_UNTER_UNTER_BEDEUTUNG" | "U3_BEDEUTUNG" ) [LZ]
                       ¬ßBedeutungsangabe (BelegeOderAufz√§hlung|{[VERBOTENER_ABSATZ] U4Bedeutung}+|FehlendeBelege) &Anschluss
